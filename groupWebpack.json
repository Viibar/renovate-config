{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "webpack",
      "matchSourceUrlPrefixes": [
        "https://github.com/webpack/",
        "https://github.com/webpack-contrib/"
      ],
      "matchPackagePatterns": ["\\w+-loader$", "\\w+-webpack-plugin$"]
    }
  ]
}
